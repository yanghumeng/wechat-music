/* components/common/musicheader/musicheader.wxss */
@import "/static/iconfont/font.wxss";
.pageheader{
  position: relative;
  height: 80rpx;
  width: 100%;
  top: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: #ffffff;
}
.backToHome {
  height: 80rpx;
  width: 80rpx;
  color: #ffffff;
  position: absolute;
  background-color:rgba(0, 0, 0, 0.5);
  line-height: 80rpx;
  text-align: center;
  border-radius: 50%;
  margin: 0 30rpx;
}
.musicName{
  font-weight: bolder;
  color: #ffffff;
  width: 70%;
  overflow: hidden;
  margin: 0 auto;
  height: 80rpx;
  line-height: 80rpx;
}
